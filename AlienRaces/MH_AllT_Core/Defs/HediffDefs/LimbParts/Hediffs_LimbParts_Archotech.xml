<?xml version="1.0" encoding="utf-8" ?>
<Defs>
	<!-- Archotech recipe defs -->

	<RecipeDef Abstract="True" Name="ATR_SurgeryMechanicalInstall_Limb_Archotech" ParentName="ATR_SurgeryMechanicalCybertech">
		<workerClass>MSE2.Recipe_InstallArtificialBodyPartWithChildren</workerClass>
		<!-- <modExtensions>
			<li Class="MSE2.AutoRecipeUsers">
				<fleshType>ATR_AndroidTier</fleshType>
			</li>
		</modExtensions> -->
		<!-- Recipe user mod extension not required due to in-built Parent filtering? -->
	</RecipeDef>

    <!-- Archotech Arm -->
    <RecipeDef ParentName="ATR_SurgeryMechanicalInstall_Limb_Archotech">
		<defName>ATR_Install_MechanicalArchotechArm</defName>
		<label>install archotech arm</label>
		<description>Install an archotech arm.</description>
        <descriptionHyperlinks>
            <ThingDef>ArchotechArm</ThingDef>
            <HediffDef>ArchotechArm</HediffDef>
        </descriptionHyperlinks>
        <jobString>Installing archotech arm.</jobString>
        <ingredients>
            <li>
                <filter>
                    <thingDefs>
                        <li>ArchotechArm</li>
                    </thingDefs>
                </filter>
                <count>1</count>
            </li>
        </ingredients>
		<fixedIngredientFilter>
			<thingDefs>
				<li>ArchotechArm</li>
			</thingDefs>
		</fixedIngredientFilter>
		<appliedOnFixedBodyParts>
			<li>ATR_MechanicalShoulder</li>
		</appliedOnFixedBodyParts>
		<addsHediff>ArchotechArm</addsHediff>
	</RecipeDef>

